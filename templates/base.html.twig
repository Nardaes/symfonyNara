<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            <link rel="stylesheet" href="{{ asset('css/styleSignIn.css') }}">
            {{ encore_entry_link_tags('app') }}

            <!-- Custom CSS -->
            <link href="{{ asset('cssAdminMart/extra-libs/c3/c3.min.css') }}" rel="stylesheet">
            <link href="{{ asset('cssAdminMart/libs/chartist/dist/chartist.min.css') }}" rel="stylesheet">
            <link href="{{ asset('cssAdminMart/extra-libs/jvector/jquery-jvectormap-2.0.2.css') }}" rel="stylesheet" />
            <!-- Custom CSS -->
            <link href="{{ asset('dist/css/style.min.css') }}" rel="stylesheet">
        {% endblock %}


    
        

        {% block javascripts %}

            <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>


            {# admin mart#}

            <script src="{{ asset('cssAdminMart/libs/jquery/dist/jquery.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/libs/popper.js/dist/umd/popper.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/libs/bootstrap/dist/js/bootstrap.min.js') }}"></script>
            <!-- apps -->
            <!-- apps -->
            <script src="{{ asset('dist/js/app-style-switcher.js') }}"></script>
            <script src="{{ asset('dist/js/feather.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/libs/perfect-scrollbar/dist/perfect-scrollbar.jquery.min.js') }}"></script>
            <script src="{{ asset('dist/js/sidebarmenu.js') }}"></script>
            <!--Custom JavaScript -->
            <script src="{{ asset('dist/js/custom.min.js') }}"></script>
            <!--This page JavaScript -->
            <script src="{{ asset('cssAdminMart/extra-libs/c3/d3.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/extra-libs/c3/c3.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/libs/chartist/dist/chartist.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/libs/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/extra-libs/jvector/jquery-jvectormap-2.0.2.min.js') }}"></script>
            <script src="{{ asset('cssAdminMart/extra-libs/jvector/jquery-jvectormap-world-mill-en.js') }}"></script>
            <script src="{{ asset('dist/js/pages/dashboards/dashboard1.min.js') }}"></script>
            <script src="{{ asset('js/monjs.js') }}"></script>

            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>

        {% block menus %}
        <div id="main-wrapper" data-theme="light" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full" data-sidebar-position="fixed" data-header-position="fixed" data-boxed-layout="full">
            
            {% include 'top_bar.html.twig' %}
            {% include 'left_sidebar.html.twig' %}
        
            <div class="page-wrapper">
                {% block body %}{% endblock %}
            </div>
                
        </div>
        {% endblock %}

         {% block log %}{% endblock %}
    </body>
</html>
